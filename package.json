{
  "name": "rsg_chess_desktop",
  "version": "1.0.0-alpha.1",
  "description": "Cool desktop chess game... for everyone!",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "win-build": "electron-packager . --platform=win32 --arch=x64 --out=dist RSGChess",
    "linux-build": "electron-packager . RSGChess --out=dist --overwrite",
    "make-exe": "npm run win-build && node ./build/winBuild.js",
    "make-linux-installer": "npm run linux-build && node build/linuxBuild.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.1",
    "electron-installer-linux": "^2.0.0",
    "electron-installer-snap": "^2.0.1",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4"
  },
  "dependencies": {
    "rsg-chess": "^1.0.0",
    "rsg-chess-graphics": "^1.0.1"
  }
}
